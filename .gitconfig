[include]
    path = .gitconfig.local
[core]
    ignorecase = false
    autocrlf = false
    editor = vim
    pager = delta --plus-style="syntax #012800" --minus-style="normal #340001" --theme='Solarized (light)'
[interactive]
    diffFilter = delta --color-only
[pager]
    branch = false

[url "https://"]
	insteadOf = git://
[url "https://github.com/"]
	insteadof = git@github.com:
